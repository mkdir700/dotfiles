      "model": "google/gemini-3-pro-preview"
    },
    "ultrabrain": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "artistry": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
    },
    "quick": {
      "model": "openai/gpt-5.3-codex"
    },
    "unspecified-low": {
      "model": "openai/gpt-5.3-codex",
      "variant": "medium"
    },
    "unspecified-high": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high"
